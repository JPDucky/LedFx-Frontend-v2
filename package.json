  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "buildgh": "PUBLIC_URL=/LedFx-Frontend-v2/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn buildgh",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pack": "electron-builder --dir",
    "dist": "react-scripts build && electron-builder",
    "distyz": "react-scripts build && electron-builder -w zip -w portable -w nsis",
    "dist-multi": "react-scripts build && electron-builder -mwl",
    "dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "wait-on tcp:3000 && electron .",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-storybookgh": "PUBLIC_URL=/LedFx-Frontend-v2/docs build-storybook -s public",
    "build-storybookwin": "set PUBLIC_URL=/LedFx-Frontend-v2/docs && build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@storybook/addon-actions": "^6.4.9",
