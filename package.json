  "build": {
    "win": {
      "icon": "build/icon.ico",
      "artifactName": "LedFx_client-${channel}--${os}.${ext}"
    },
    "portable": {
      "artifactName": "LedFx_client-${channel}--${os}-portable.${ext}"
    },
    "nsis": {
      "artifactName": "LedFx_client-${channel}--${os}-setup.${ext}"
    },
    "linux": {
