    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "buildgh": "PUBLIC_URL=/LedFx-Frontend-v2/ react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "yarn buildgh",
        "deploy": "gh-pages -d build",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "pack": "electron-builder --dir",
        "dist": "react-scripts build && electron-builder",
        "dist-multi": "react-scripts build && electron-builder -mwl",
        "dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
        "electron": "wait-on tcp:3000 && electron .",
        "postinstall": "electron-builder install-app-deps"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@electron-forge/maker-deb": "^6.0.0-beta.61",
        "@electron-forge/maker-rpm": "^6.0.0-beta.61",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
        "@electron-forge/maker-zip": "^6.0.0-beta.61",
        "@types/react-router-dom": "^5.3.0",
        "@typescript-eslint/eslint-plugin": "^4.31.2",
        "@typescript-eslint/parser": "^4.31.2",
        "babel-eslint": "^10.1.0",
        "concurrently": "^6.2.1",
        "cross-env": "^7.0.3",
        "electron": "^14.0.1",
        "electron-builder": "^22.11.7",
        "electron-devtools-installer": "^3.2.0",
        "eslint": "^7.27.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-flowtype": "^6.1.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-json": "^3.0.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.0",
        "eslint-plugin-react-hooks": "^4.0.8",
        "gh-pages": "^3.2.0",
        "prettier": "^2.3.0",
        "react-is": "^17.0.2",
        "wait-on": "^6.0.0"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "src/icons/mac/icon.icns",
                "name": "LedFx Client by Blade",
                "title": "LedFx Client by Blade"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "ledfx"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "build": {
        "win": {
            "icon": "build/icon.ico"
        },
        "linux": {
            "icon": "build/icon.png"
        },
        "mac": {
            "icon": "build/icon.png"
        }
    }
}
